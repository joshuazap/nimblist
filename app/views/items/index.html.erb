<div class="col-md-10">
  <h2>My To-Do List</h2>
    <table class="table table-hover">
      <tr>
        <th>Item</th>
        <th>Days Left</th>
        <th>Status</th>
      </tr>
      <tbody class="js-items">
        <%= render current_user.items %>
      </tbody>
    </table>
<div class='new-item'>
  <%= render partial: 'items/form', locals: { item: Item.new } %>
</div>